{
  "name": "template-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://frontkid.github.io/template-vite",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --ignore-path .gitignore --report-unused-disable-directives",
    "format": "prettier . --ignore-path .prettierignore --write",
    "fix-style": "npm run format & npm run lint",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fileforge/client": "^0.1.18",
    "@fileforge/react-print": "^0.1.144",
    "@mui/material": "^5.16.7",
    "@mui/x-date-pickers": "^7.8.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/node": "^20.12.12",
    "@uidotdev/usehooks": "^2.4.1",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "firebase": "^10.12.2",
    "formik": "^2.4.6",
    "gh-pages": "^6.1.1",
    "node-fetch": "^2.6.9",
    "react": "^18.2.0",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "rollup-plugin-node-externals": "^7.1.3",
    "uuid": "^9.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "^1.0.13",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/react": "^18.2.43",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "3.2.4",
    "sass": "^1.70.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-node-polyfills": "^0.22.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run fix-style --exit-on-error"
  }
}
